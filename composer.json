{
  "name": "codexten/yii-mail-aws",
  "type": "yii2-extension",
  "description": "yii aws",
  "keywords": [
    "yiisoft",
    "aws",
    "codexten",
    "yii2"
  ],
  "require": {
    "codexten/yii-aws": ">=2.0",
    "ofat/yii2-yashop-ses": "*",
    "codexten/yii-mailqueue": "2.0.*"
  },
  "autoload": {
    "psr-4": {
      "codexten\\yii\\mail\\aws\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    },
    "config-plugin": {
      "mailer": "config/mailer.php",
      "params": "config/params.php"
    }
  }
}
